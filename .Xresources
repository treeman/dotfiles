#ifdef COLOR
*customization: -color
#endif

! Blink
*visualBell: False

Xft.antialias: 1
#Xft.hinting: -1
Xft.hinting: 1
#Xft.hintstyle: hintslight
Xft.hintstyle: hintfull
#Xft.hintstyle: hintmedium
Xft.lcdfilter: lcddefault
Xft.autohint: 0
Xft.dpi: 96

! URxvt.urgentOnBell: true
URxvt.urgentOnBell: false

