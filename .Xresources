#ifdef COLOR
*customization: -color
#endif

! Blink
!*visualBell: False

Xft.antialias: 1
Xft.hinting: -1
Xft.rgba: rgb
Xft.hintstyle: hintslight
Xft.dpi: 100

URxvt.urgentOnBell: true

! irssi notifications
XTerm*bellIsUrgent: true

XTerm*internalBorder: 0
